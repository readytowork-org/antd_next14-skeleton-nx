{
  "name": "consumer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/consumer",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "nextConfig": "apps/consumer/next.config.js",
        "root": "apps/consumer",
        "outputPath": "dist/apps/consumer/"
      },
      "configurations": {
        "production": {
          "optimizations": true,
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": true
        }
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "consumer:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "dev": false,
          "buildTarget": "consumer:build:production"
        },
        "development": {
          "dev": true,
          "buildTarget": "consumer:build:development"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/consumer/jest.config.ts"
      }
    }
  }
}
